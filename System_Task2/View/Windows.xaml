<Window x:Class="System_Task2.View.Windows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:System_Task2.View"
        mc:Ignorable="d"
        Title="Process" Height="250" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <WrapPanel>
            <Button Command="{Binding processCommand}" Width="190" Height="30">Process</Button>
            <Button Command="{Binding logCommand}" Width="190"  Height="30">Log File</Button>
            <Button Command="{Binding configurationCommand}" Width="190" Height="30" >Configuration</Button>
        </WrapPanel>
        <Grid Grid.Row="1" Name="Grid">
            
        </Grid>
        
    </Grid>
</Window>
